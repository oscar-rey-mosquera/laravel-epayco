{
    "name": "oscar-rey/laravel-epayco",
    "description": "Wrapper de epayco sdk para laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
      "epayco",
      "laravel",
      "sdk",
      "wrapper",
      "epayco-sdk",
      "epayco-wrapper",
      "epayco-laravel",
      "Epayco",
      "Davivienda"
    ],
    "autoload": {
        "psr-4": {
            "LaravelEpayco\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
          "LaravelEpayco\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Oscar Rey",
            "email": "oscarhumberto1311@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^7.5"
    },
    "require": {
        "epayco/epayco-php": "^1.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEpayco\\EpaycoServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --stderr"
    }
}
